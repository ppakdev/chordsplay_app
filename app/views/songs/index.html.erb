<h1>Listing songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Chords</th>
      <th>Body</th>
      <th>Year</th>
      <th>Youtube url</th>
      <th>Soundcloud url</th>
      <th>Ug url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= song.chords %></td>
        <td><%= song.body %></td>
        <td><%= song.year %></td>
        <td><%= song.youtube_url %></td>
        <td><%= song.soundcloud_url %></td>
        <td><%= song.ug_url %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
